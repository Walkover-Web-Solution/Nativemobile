    "copy-webpack-plugin": "4.3.0",
    "cross-env": "5.1.1",
    "css-loader": "0.28.7",
    "del-cli": "1.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "lazy": "1.0.11",
    "nativescript-css-loader": "0.26.1",
    "nativescript-dev-appium": "next",
    "nativescript-dev-sass": "1.3.5",
    "nativescript-dev-typescript": "0.6.0",
    "nativescript-dev-webpack": "^0.9.0",
    "nativescript-worker-loader": "0.8.1",
    "node-sass": "4.7.1",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "2.1.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "tns-platform-declarations": "3.4.0",
    "typescript": "2.6.2",
    "uglifyjs-webpack-plugin": "1.1.5",
    "webpack": "3.10.0",
    "webpack-bundle-analyzer": "2.8.2",
    "webpack-parallel-uglify-plugin": "1.0.2",
    "webpack-sources": "1.1.0",
    "awesome-typescript-loader": "~3.1.3"
  },
  "scripts": {
    "u": "update-ns-webpack",
    "start": "cross-env BUILD_PROFILE=test tns debug android --chrome ",
    "start:android": "cross-env BUILD_PROFILE=dev tns run android --chrome",
    "start:ios": "cross-env BUILD_PROFILE=test tns run ios",
    "debug:android": "cross-env BUILD_PROFILE=test tns debug android --chrome",
    "debug:ios": "cross-env BUILD_PROFILE=test tns debug ios",
    "debug": "cross-env BUILD_PROFILE=dev tns debug ios --chrome",
    "clean": "npm run js.clean && npm run css.clean",
    "css.clean": "npx del '*.css' '!font-awesome.min.css'",
    "css.generate": "node -e 'require(\"./node_modules/nativescript-dev-sass/lib/converter.js\").convert(console, \".\");'",
    "css": "npm run css.clean && npm run css.generate",
    "js.clean": "npx rimraf -- app/**/*.js",
    "ns-bundle": "ns-bundle",
    "start-android-bundle": "cross-env BUILD_PROFILE=test tns run android --bundle",
    "start-ios-bundle": "cross-env BUILD_PROFILE=test tns run ios --bundle --env.aot --env.uglify | tee log.txt ",
    "build-android-bundle": "cross-env BUILD_PROFILE=prod tns build android --compileSdk 25 --bundle --release --key-store-path ./giddh.keystore --key-store-password asd123 --key-store-alias mykey --key-store-alias-password asd123",
    "build-ios-bundle": "cross-env BUILD_PROFILE=prod tns build ios     --release --for-device --provision a1a56f83-05bf-4ae3-8d53-09e2b45cbd02 --bundle --env.aot --env.uglify",
    "prepare-android-bundle": "cross-env BUILD_PROFILE=prod tns prepare android  --bundle  --env.aot --env.uglify",
    "prepare-ios-bundle": "cross-env BUILD_PROFILE=prod tns prepare ios --release --for-device --team-id F3U6Z5L2EJ --provision a1a56f83-05bf-4ae3-8d53-09e2b45cbd02 --bundle --env.aot --env.uglify",
    "publish-ios-bundle": "cross-env BUILD_PROFILE=prod tns publish ios --release --for-device --team-id F3U6Z5L2EJ --provision a1a56f83-05bf-4ae3-8d53-09e2b45cbd02 --bundle --env.aot --env.uglify",
    "e2e": "tsc -p e2e && mocha --opts ./e2e/config/mocha.opts",
    "compile-tests": "tsc -p e2e --watch"
  }
}
