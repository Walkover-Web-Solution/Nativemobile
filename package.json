  "readme": "NativeScript Application",
  "repository": "<fill-your-repository-here>",
  "nativescript": {
    "id": "com.giddh.accounts",
    "name": "giddh",
    "hooks": [
      {
        "type": "before-prepare",
        "script": "./customHooks/before-prepare/nativescript-environment.js"
      }
    ],
    "tns-ios": {
      "version": "3.4.0"
    },
    "tns-android": {
      "version": "3.4.1"
    }
  },
  "dependencies": {
    "@angular/animations": "5.0.0",
